:execute pathogen#infect()

:colorscheme desert
:syntax on
:set guifont=Droid\ Sans\ Mono\ 16
:set nobackup
:set nowritebackup
:set tabstop=4
:set shiftwidth=4
:set expandtab
:set mouse=a
:map <F2> ggVG"+y
:map <F3> "+yiw

:filetype off
:filetype plugin indent on
:filetype plugin on
:filetype indent on

